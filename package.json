{
    "name": "lab3web",
    "version": "1.0.0",
    "private": true,
    "dependencies": {
        "@apollo/client": "^3.4.17",
        "@apollo/react-hooks": "^4.0.0",
        "apollo-utilities": "^1.3.4",
        "dotenv": "^10.0.0",
        "eslint-import-resolver-webpack": "^0.13.2",
        "graphql": "^15.7.2",
        "react": "^17.0.2",
        "react-bootstrap": "^2.0.2",
        "react-dom": "^17.0.2",
        "react-scripts": "4.0.3",
        "sonarjs": "^1.0.0",
        "subscriptions-transport-ws": "^0.11.0",
        "web-vitals": "^1.1.2"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "lint": "npx eslint **/*.js --ignore-path .gitignore --no-error-on-unmatched-pattern",
        "fix": "npx eslint --fix **/*.js --ignore-path .gitignore --no-error-on-unmatched-pattern",
        "fmt": "npx prettier --write . --ignore-path .gitignore",
        "fmt:check": "npx prettier --check . --ignore-path .gitignore",
        "heroku-postbuild": "npm run build",
        "prepare": "husky install"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^5.15.0",
        "@testing-library/react": "^11.2.7",
        "@testing-library/user-event": "^12.8.3",
        "apollo-boost": "^0.4.9",
        "apollo-link": "^1.2.14",
        "apollo-link-ws": "^1.0.20",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.25.3",
        "eslint-plugin-jsx-a11y": "^6.5.1",
        "eslint-plugin-react": "^7.27.0",
        "eslint-plugin-react-hooks": "^4.3.0",
        "graphql-tag": "^2.12.6",
        "husky": "^7.0.4",
        "prettier": "2.4.1"
    },
    "homepage": "https://lab3-full-version.herokuapp.com/"
}
